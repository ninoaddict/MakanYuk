        .categories {
            margin: 15vh auto 80px auto;
            width: 100%;
            display: flex;
            justify-content: center;
        }

        .categories-wrapper {
            display: flex;
            flex-direction: column;
            width: 692px;
            gap: 28px;
        }

        .category-tag {
            width: 175px;
            height: 39px;
            flex-shrink: 0;
            fill: #ECF2EB;
            border-radius: 30px;
            box-shadow: 0px 4px 4px 0px rgba(0, 0, 0, 0.25) inset;
            text-align: center;
            color: #000;
            font-feature-settings: 'clig' off, 'liga' off;
            font-family: Poppins;
            font-size: 21.961px;
            font-style: normal;
            font-weight: 500;
            line-height: 150%;
            /* 32.941px */
        }

        .category-container {
            box-sizing: border-box;
            width: 692px;
            height: 509px;
            flex-shrink: 0;
            border-radius: 30px;
            background: #F2F2F2;
            box-shadow: 0px 0px 10px 4px rgba(0, 0, 0, 0.25) inset;
            display: grid;
            gap: 56px;
            grid-template-columns: 1fr 1fr 1fr;
            padding: 56px;
        }

        .category-item {
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            width: 156px;
            height: 148px;
            border-radius: 30px;
            background: #FFF;
            box-shadow: 2px 4px 4px 2px rgba(0, 0, 0, 0.25);
